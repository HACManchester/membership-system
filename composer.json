{
    "name": "hacmanchester/membership-system",
    "description": "Hackspace Manchester Member System",
    "authors": [
        {
            "name": "Hackspace Manchester",
            "email": "board@hacman.org.uk"
        }
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "arthurguy/notifications": "~1.0",
        "fideloper/proxy": "^4.0",
        "gocardless/gocardless-pro": "~1.1",
        "guzzlehttp/guzzle": "^6.5",
        "guzzlehttp/psr7": "^1.9",
        "intervention/image": "2.*",
        "itsgoingd/clockwork": "~1.8",
        "laracasts/presenter": "~0.2",
        "laravel-notification-channels/telegram": "^0.0.5",
        "laravel/framework": "5.8.*",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.8",
        "michelf/php-markdown": "~1.4",
        "nesbot/carbon": "^2.0.0",
        "owen-it/laravel-auditing": "^9.0",
        "rap2hpoutre/laravel-log-viewer": "^2.3",
        "sentry/sentry-laravel": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0",
        "mockery/mockery": "^1.0",
        "fzaninotto/faker": "^1.4",
        "barryvdh/laravel-ide-helper": "^2.4",
        "symfony/css-selector": "~4.0",
        "symfony/dom-crawler": "~4.0",
        "laravel/browser-kit-testing": "^5.1.0",
        "filp/whoops": "~2.0",
        "doctrine/dbal": "~2.6",
        "nunomaduro/collision": "^2.0",
        "beyondcode/laravel-dump-server": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "BB\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [],
        "pre-update-cmd": [],
        "post-update-cmd": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "post-create-project-cmd": [
            "php -r \"copy('.env.example', '.env');\"",
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "allow-plugins": {
            "php-http/discovery": true,
            "kylekatarnls/update-helper": true
        },
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    }
}
